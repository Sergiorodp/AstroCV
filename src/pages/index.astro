---
//Layout
import SergiLayout from '../layouts/SergiNewLayout.astro';

// Compose Components
import SergiNav from '../components/SergiComposeComponents/SergiNewNav.astro'
import SergiHome from "../components/SergiComposeComponents/SergioHome.astro";
import SergiContact from "../components/SergiComposeComponents/SergiContact.astro";
import SergiAboutMe from '../components/SergiComposeComponents/SergiAboutMe.astro';
---
<SergiLayout title='HelloAstro'>
  <SergiContact slot='static-links-slot'></SergiContact>
  <SergiNav slot='header-slot'></SergiNav>
  <SergiHome></SergiHome>
  <SergiAboutMe></SergiAboutMe>
</SergiLayout>

<style lang="scss">
  div{
    color: azure;
    grid-column: 1/4;
  }
</style>